{
  "name": "sadi01/yii2-more-settings",
  "description": "Handle Settings For Yii2 Framework",
  "keywords": [
    "yii2",
    "framework",
    "settings"
  ],
  "homepage": "https://github.com/Sadi01/yii2-more-settings",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/Sadi01/yii2-more-settings/issues?state=open",
    "source": "https://github.com/Sadi01/yii2-more-settings"
  },
  "authors": [
    {
      "name": "Sadegh Shafii",
      "email": "sadshafiei.01@gmail.com",
      "homepage": "https://github.com/Sadi01"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "yiisoft/yii2": "~2.0.14",
    "kartik-v/yii2-grid": "@dev",
    "kartik-v/yii2-widget-fileinput": "@dev",
    "kartik-v/yii2-number": "@dev",
    "sadi01/yii2-daterangepicker": "*",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": { "sadi01\\moresettings\\": "src" }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "bootstrap": "sadi01\\moresettings\\Bootstrap"
  }
}